  <select id="query_element_product" resultClass="com.focus3d.pano.model.Product" parameterClass="com.focus3d.pano.model.ibator.PanoPerspectiveElementModel">
      SELECT 
	   * 
	  FROM
	  	pano_perspective_element element 
	  LEFT JOIN pano_project_package_product packageProduct 
	    ON element.`PACKAGE_TYPE_SN` = packageProduct.`PACKAGE_TYPE_SN` 
	  RIGHT JOIN pano_product product 
	    ON product.`SN` = packageProduct.`PRODUCT_SN` 
	  WHERE element.`SN` = #sn# 
  </select>
  
  <select id="query_element" resultClass="java.util.HashMap" parameterClass="com.focus3d.pano.model.ibator.PanoPerspectiveElementModel">
    SELECT 
	  element.sn AS id,
	  element.`ELEMENT_NAME` AS name,
	  element.`ELEMENT_ORDER` AS elementOrder 
	FROM
	  pano_perspective_element element 
	  LEFT JOIN pano_perspective_element_product elementProduct 
	    ON element.ELEMENT_PRODUCT_SN = elementProduct.sn 
	WHERE element.`LAYER_SN` = #layerSn# 
  </select>
  
